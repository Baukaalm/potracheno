  "scripts": {
    "start": "node ./server.js",
    "start:production": "npm run clean && npm run copy-static && cross-env NODE_ENV=production node ./server.js",
    "start:storybook": "start-storybook -p 9001 -c .storybook",
    "clean": "rm -rf build/",
    "copy-static": "cp -R static build",
    "build": "npm run clean && npm run copy-static && cross-env NODE_ENV=production webpack --progress --colors && npm run build:zip",
    "build:zip": "rm -rf ./build.zip && cd ./build && ls -A | zip -@ build.zip",
    "qa": "npm run lint && npm run test",
    "lint": "eslint source/scripts --ext .js",
    "test": "jest",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": "lint",
    "*": "test"
  },
